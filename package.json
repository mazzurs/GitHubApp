  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.8",
    "@fortawesome/react-fontawesome": "0.0.20",
    "@material/button": "^0.35.0",
    "material-ui": "^0.20.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-infinite-scroller": "^1.1.4",
    "react-paginate": "^5.2.3",
    "react-redux": "^5.0.7",
